<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="br.edu.ifsc.fln.controller.FXMLAchorPaneCadastroMarcaDialogController">
   <children>
      <GridPane layoutX="80.0" layoutY="116.0" prefHeight="126.0" prefWidth="441.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="221.0" minWidth="10.0" prefWidth="221.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="220.0" minWidth="10.0" prefWidth="220.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="221.0" text="Descrição: " />
            <TextField fx:id="tfDescricao" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Group layoutX="395.0" layoutY="321.0">
         <children>
            <Button fx:id="btConfirmar" mnemonicParsing="false" onAction="#handleBtConfirmar" text="Confirmar" />
            <Button fx:id="btCancelar" layoutX="79.0" mnemonicParsing="false" onAction="#handleBtCancelar" prefHeight="25.0" prefWidth="66.0" text="Cancelar" />
         </children>
      </Group>
